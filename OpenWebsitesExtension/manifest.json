{
  "manifest_version": 3,
  "name": "Website Launcher",
  "version": "1.1",
  "description": "Save and open your favorite websites with one click.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  }
}
